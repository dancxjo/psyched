# Pilot module configuration (env)
PILOT_WEB_PORT=8080
PILOT_WS_PORT=8081
PILOT_HOST=0.0.0.0
PILOT_CMD_VEL_TOPIC=/cmd_vel
PILOT_VOICE_TOPIC=/voice
# Enable HTTP server in pilot_node (true/false)
PILOT_ENABLE_HTTP=true
# Enable WebSocket inside pilot_node (use false when running separate websocket node)
PILOT_ENABLE_WS=false
# Launch the separate websocket node
PILOT_RUN_SEPARATE_WS=true
