<?xml version="1.0"?>
<package format="3">
  <name>pilot</name>
  <version>0.2.0</version>
  <description>Pilot module cockpit backend implemented with rclpy and websockets.</description>
  <maintainer email="pete@psyched.local">Psyched Maintainers</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>rclpy</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>nav_msgs</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>create_msgs</exec_depend>
  <exec_depend>python3-websockets</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
