<?xml version="1.0"?>
<package format="3">
  <name>faces</name>
  <version>0.1.0</version>
  <description>Face detection and embedding pipeline for the Psyched robots.</description>
  <maintainer email="devnull@example.com">Psyched Dev</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_python</buildtool_depend>
  <build_depend>faces_msgs</build_depend>

  <exec_depend>faces_msgs</exec_depend>
  <exec_depend>rclpy</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>cv_bridge</exec_depend>
  <exec_depend>opencv-python</exec_depend>
  <exec_depend>numpy</exec_depend>

  <test_depend>pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
