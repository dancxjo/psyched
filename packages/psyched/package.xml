<?xml version="1.0"?>
<package format="3">
    <name>psyched</name>
    <version>0.1.0</version>
    <description>Psyched example package with Python and C++ nodes</description>
    <maintainer email="you@example.com">Psyched</maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <depend>rclcpp</depend>
    <depend>rclpy</depend>
    <depend>std_msgs</depend>

    <test_depend>ament_lint_auto</test_depend>
    <test_depend>ament_lint_common</test_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
